{
    "extends": [
        "tslint-config-airbnb"
    ],
    "rules": {
      "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
      "max-line-length": [
        true, 
       { 
           "limit": 120, 
           "ignore-pattern": "^import |RegExp" 
       }
   ]
    }
}